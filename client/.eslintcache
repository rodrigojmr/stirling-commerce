   {title && (\r\n          <Heading as=\"h1\" fontSize=\"3xl\">\r\n            {title}\r\n          </Heading>\r\n        )}\r\n        <Toggle\r\n          checked={isChecked}\r\n          id=\"recommendedProduct\"\r\n          onChange={setIsChecked}\r\n          optionLabels={['Men', 'Women']}\r\n        />\r\n      </Flex>\r\n      <SwitchTransition mode=\"out-in\">\r\n        <FadeTransition\r\n          key={isChecked ? 'firstComponent' : 'secondComponent'}\r\n          timeout={100}\r\n        >\r\n          {isChecked ? children[1] : children[0]}\r\n        </FadeTransition>\r\n      </SwitchTransition>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ToggleSlider;\r\n","/home/rodrigo/projects/personal-projects/gym-ecommerce/client/src/utils/privateRoute.tsx",[],"/home/rodrigo/projects/personal-projects/gym-ecommerce/client/src/components/toggle.tsx",["379"],"import React from 'react';\r\nimport styled from '@emotion/styled';\r\nimport { Box, Input, Text } from '@chakra-ui/react';\r\n\r\nconst Wrapper = styled.div`\r\n  position: relative;\r\n`;\r\n\r\nconst ToggleWrapper = styled.div`\r\n  display: flex;\r\n  align-items: center;\r\n\r\n  > *:first-child {\r\n    margin-right: 1.8rem;\r\n  }\r\n  > *:last-child {\r\n    margin-left: 1.8rem;\r\n  }\r\n`;\r\n\r\nconst StyledLabel = styled.label`\r\n  position: relative;\r\n  display: inline-block;\r\n  width: 6rem;\r\n  font-size: 0;\r\n  padding: 5px;\r\n  border: 3px solid black;\r\n  border-radius: 3rem;\r\n  outline: none;\r\n  &:focus,\r\n  &:active {\r\n    filter: brightness(1.3);\r\n    box-shadow: 0px 0px 15px 7px #78b2ff;\r\n  }\r\n`;\r\n\r\nconst SwitchWrapper = styled.div<{ checked: boolean }>`\r\n  width: 100%;\r\n  height: 1.6rem;\r\n  transition: 0.25s all ease-in-out;\r\n  transform: ${({ checked }) => `translateX(${checked ? '100%' : '0%'})`};\r\n`;\r\n\r\nconst StyledSwitch = ({ checked }: { checked: boolean }) => (\r\n  <Box\r\n    as=\"span\"\r\n    display=\"inline-block\"\r\n    width=\"2.2rem\"\r\n    height=\"1.6rem\"\r\n    borderRadius=\"2rem\"\r\n    bg=\"primary.500\"\r\n    transition=\"0.25s all ease-in-out\"\r\n    transform={checked ? 'translateX(-100%)' : 'translateX(0%)'}\r\n  ></Box>\r\n);\r\nconst OptionLabel = styled.p`\r\n  font-family: 'Bebas Neue';\r\n  font-size: 2rem;\r\n  color: black;\r\n`;\r\n\r\ninterface Props {\r\n  id: string;\r\n  checked: boolean;\r\n  onChange: (checked: boolean) => void;\r\n  optionLabels: readonly [string, string];\r\n}\r\n\r\nconst Toggle: React.FC<Props> = ({ id, checked, onChange, optionLabels }) => {\r\n  const handleKeyPress = (e: React.KeyboardEvent) => {\r\n    console.log(e);\r\n    if (e.keyCode !== 32) return;\r\n\r\n    e.preventDefault();\r\n    onChange(!checked);\r\n  };\r\n\r\n  return (\r\n    <ToggleWrapper>\r\n      <Text fontSize=\"xl\" fontWeight=\"600\" fontFamily=\"heading\">\r\n        {optionLabels[1]}\r\n      </Text>\r\n      <Wrapper>\r\n        <Input\r\n          type=\"checkbox\"\r\n          style={{ display: 'none' }}\r\n          id={id}\r\n          checked={checked}\r\n          onChange={e => onChange(e.target.checked)}\r\n        />\r\n        {id ? (\r\n          <StyledLabel\r\n            className=\"toggle-switch-label\"\r\n            tabIndex={0}\r\n            onKeyDown={e => handleKeyPress(e)}\r\n            htmlFor={id}\r\n          >\r\n            <SwitchWrapper checked={checked}>\r\n              <StyledSwitch checked={checked} />\r\n            </SwitchWrapper>\r\n          </StyledLabel>\r\n        ) : null}\r\n      </Wrapper>\r\n      <Text fontSize=\"xl\" fontWeight=\"600\" fontFamily=\"heading\">\r\n        {optionLabels[1]}\r\n      </Text>\r\n    </ToggleWrapper>\r\n  );\r\n};\r\n\r\nexport default Toggle;\r\n","/home/rodrigo/projects/personal-projects/gym-ecommerce/client/src/components/products/highlightDot.tsx",[],"/home/rodrigo/projects/personal-projects/gym-ecommerce/client/src/pages/productPage.tsx",["380","381"],"/home/rodrigo/projects/pe